Switch BackLight       "Front Room Back Lights"	    	(group)	{ http=">[ON:GET:http://unipi:8080/CMD?backLight=ON] >[OFF:GET:http://unipi:8080/CMD?backLight=OFF]", autoupdate="false" }

Switch PowerCeiling    "Front Room Power Ceiling"	 	(group)	{ http=">[ON:GET:http://unipi:8080/CMD?powerCeiling=ON]", autoupdate="false" }
Switch PowerWall       "Front Room Power Wall"	 	    (group)	{ http=">[ON:GET:http://unipi:8080/CMD?powerWall=ON]", autoupdate="false" }
